<h1><%= action_message %></h1>

<div class="row">

  <% if @test.errors.any? %>
    <div class="alert alert-danger alert-dismissable">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">
        <ul>
          <% @test.errors.full_messages.each do |message|%>
             <li> <%= message %> </li>
          <% end%>
         
        </ul>
      </button>
    </div>
  <% end %>

  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
       
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-lg-6">


            <%= form_with(model: [ :admins_backoffice, @test], local: true) do |form| %>

              <%= %>
              <div class="form-group">
                <%= form.label :name, "Nome da Avaliação"%>
                <%= form.text_field :test_name, class:"form-control" %>
          
              </div>

              <div class="form-group">
              
                <%= form.label :subject,  "Assunto" %> </br>
                <%= form.collection_select(:subject_id, @subjects, :id, :description) %>
            
              </div> 

              
              <div id='questions' class="form-group">
                
                <%= form.label :description, "Pergunta" %> </br>
                <%= form.collection_select(:question_id, @questions, :id, :description) %> 

                <%= link_to "Buscar Questoes", class:"btn btn-primary mt-5" %>
              
              </div>

             <button type="submit" class="btn btn-default">Salvar</button>
           <% end %>
          </div>
          <!-- /.col-lg-6 (nested) -->
        </div>
        <!-- /.row (nested) -->
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>
  <!-- /.col-lg-12 -->
</div>