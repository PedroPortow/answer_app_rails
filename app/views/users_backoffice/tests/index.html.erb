<h1>Avaliações</h1>
 
<div class="row">
  <div class="animated flipInY col-lg-12 col-md-3 col-sm-6 col-xs-12">
    <table class="table table-hover">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">Avaliação</th>
          <th scope="col">Respondida em</th>
          <th scope="col">Nota</th>
        </tr>
      </thead>
      <tbody>
        <% @tests.each do |test| %>
          <th scope="row"> <%= test.id  %></th>
          <td><%= link_to test.name, users_backoffice_path(test.id), class:'btn btn-primary' %></td>
          <td>TODO</td>
          <td>TODO</td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  
</div>
